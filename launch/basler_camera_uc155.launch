<launch>
  <arg name="cam_width" default="1920" />
  <arg name="cam_height" default="1080" />
  <arg name="cam_model" default="acA1920-155uc" />
  <arg name="camera_frame" default="acA1920-155uc" />

  <!-- Choose the calibration file that corresponds to the resolution being used. -->
  <arg name="cam_calibration_file" default="file://$(find basler_camera)/config/$(arg cam_model)_$(arg cam_width)x$(arg cam_height).yaml" />

  <node name="acA1920_155uc" pkg="basler_camera" type="basler_camera_node" output="screen">
    <!--param name="frame_rate" type="int" value="2" / Changed in dart_sync_master.yaml instead-->
    <param name="frame_id" type="string" value="$(arg camera_frame)" />
    <param name="camera_info_url" type="string" value="$(arg cam_calibration_file)" />
    <param name="serial_number" type="string" value="21764272"/>
    <rosparam command="load" file="$(find basler_camera)/config/dart_sync_master.yaml"/>
  </node>

  <node ns="acA1920_155uc" pkg="image_proc" type="image_proc" name="image_proc"/>
</launch>



